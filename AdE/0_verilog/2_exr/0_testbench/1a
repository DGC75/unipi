#! /usr/bin/vvp
:ivl_version "10.3 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "vhdl_sys";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0x55bdc8b51f20 .scope module, "test" "test" 2 18;
 .timescale 0 0;
v0x55bdc8b60cd0_0 .net "out", 0 0, L_0x55bdc8b60d70;  1 drivers
S_0x55bdc8b520a0 .scope module, "t_clock" "clock" 2 22, 3 1 0, S_0x55bdc8b51f20;
 .timescale 0 0;
    .port_info 0 /OUTPUT 1 "out"
L_0x55bdc8b60d70 .functor BUFZ 1, v0x55bdc8b1c080_0, C4<0>, C4<0>, C4<0>;
v0x55bdc8b1c080_0 .var "clock", 0 0;
v0x55bdc8b60bd0_0 .net "out", 0 0, L_0x55bdc8b60d70;  alias, 1 drivers
    .scope S_0x55bdc8b520a0;
T_0 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55bdc8b1c080_0, 0, 1;
    %end;
    .thread T_0;
    .scope S_0x55bdc8b520a0;
T_1 ;
    %delay 1, 0;
    %load/vec4 v0x55bdc8b1c080_0;
    %inv;
    %store/vec4 v0x55bdc8b1c080_0, 0, 1;
    %jmp T_1;
    .thread T_1;
    .scope S_0x55bdc8b51f20;
T_2 ;
    %vpi_call 2 26 "$monitor", "%t %d", $time, v0x55bdc8b60cd0_0 {0 0 0};
    %vpi_call 2 28 "$dumpfile", "clock.vcd" {0 0 0};
    %vpi_call 2 29 "$dumpvars" {0 0 0};
    %delay 30, 0;
    %vpi_call 2 33 "$finish" {0 0 0};
    %end;
    .thread T_2;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "1_test.v";
    "0_clock.v";
